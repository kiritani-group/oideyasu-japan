generator client {
  provider               = "prisma-client"
  output                 = "../lib/generated/prisma"
  engineType             = "client"
  moduleFormat           = "esm"
  generatedFileExtension = "ts"
  importFileExtension    = "ts"
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_DATABASE_URL")
}
